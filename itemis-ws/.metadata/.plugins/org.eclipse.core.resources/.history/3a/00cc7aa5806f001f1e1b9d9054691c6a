testclass LabRecruitsSimplifiedTest for statechart LabRecruits {
	@Test
	operation test00 () {
	    triggerWithoutEvent
	    assert !is_active
	}

	@Test
	operation test01 () {
	    raise travel_to_d2p
	    assert !is_final
	}

	@Test
	operation test02 () {
	    raise travel_to_d2m
	    assert !is_final
	}

	@Test
	operation test03 () {
	    raise travel_to_b2
	    assert !is_active
	}

	@Test
	operation test04 () {
	    assert !is_active
	    enter
	    raise travel_to_d3m
	    raise travel_to_b0
	    assert is_active
	}

	@Test
	operation test05 () {
	    assert !is_active
	    enter
	    raise travel_to_d3m
	    raise travel_to_d1m
	    assert is_active
	}

	@Test
	operation test06 () {
	    assert !is_active
	    enter
	    raise travel_to_d3m
	    raise travel_to_b1
	    assert is_active
	}

	@Test
	operation test07 () {
	    assert !is_active
	    enter
	    raise travel_to_b1
	    raise toggle
	    raise travel_to_d1m
	    raise travel_to_d1p
	    raise travel_to_d3m
	    assert is_active
	}

	@Test
	operation test08 () {
	    assert !is_active
	    enter
	    raise travel_to_d1m
	    raise travel_to_b1
	    assert is_active
	}

	@Test
	operation test09 () {
	    assert !is_active
	    enter
	    raise travel_to_d1m
	    raise travel_to_d3m
	    assert is_active
	}

	@Test
	operation test10 () {
	    assert !is_active
	    enter
	    raise travel_to_d1m
	    raise travel_to_b0
	    assert is_active
	}

	@Test
	operation test11 () {
	    assert !is_active
	    enter
	    raise travel_to_d1m
	    raise travel_to_b3
	    assert is_active
	}

	@Test
	operation test12 () {
	    assert !is_active
	    enter
	    raise travel_to_b1
	    raise travel_to_d3p
	    assert is_active
	}

	@Test
	operation test13 () {
	    assert !is_active
	    enter
	    raise travel_to_b1
	    raise travel_to_b0
	    assert is_active
	}

	@Test
	operation test14 () {
	    assert !is_active
	    enter
	    raise travel_to_b1
	    raise travel_to_d3m
	    assert is_active
	}

	@Test
	operation test15 () {
	    assert !is_active
	    enter
	    raise travel_to_b0
	    assert is_active
	}

	@Test
	operation test16 () {
	    assert !is_active
	    enter
	    raise toggle
	    assert is_active
	}

	@Test
	operation test17 () {
	    assert !is_active
	}

	@Test
	operation test18 () {
	    assert !is_active
	}

	@Test
	operation test19 () {
	    assert !is_active
	}

	@Test
	operation test20 () {
	    assert !active (LabRecruits.main_region.d1m)
	    assert !is_active
	}

	@Test
	operation test21 () {
	    assert !active (LabRecruits.main_region.d1p)
	    assert !is_active
	}

	@Test
	operation test22 () {
	    assert !active (LabRecruits.main_region.d2m)
	    assert !is_active
	}

	@Test
	operation test23 () {
	    assert !active (LabRecruits.main_region.b3)
	    assert !is_active
	}

	@Test
	operation test24 () {
	    assert !is_active
	    assert !active (LabRecruits.main_region.b2)
	}

	@Test
	operation test25 () {
	    assert !is_active
	    assert !active (LabRecruits.main_region.b1)
	}

	@Test
	operation test26 () {
	    assert !active (LabRecruits.main_region.b0)
	    assert !is_active
	}

	@Test
	operation test27 () {
	    assert !active (LabRecruits.main_region.d2p)
	    assert !is_active
	}

	@Test
	operation test28 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test29 () {
	    assert !is_active
	}

	@Test
	operation test30 () {
	    assert !is_active
	    enter
	    raise travel_to_d3m
	    raise travel_to_d3p
	    assert is_active
	}

	@Test
	operation test31 () {
	    enter
	    raise travel_to_b1
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test32 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test33 () {
	    enter
	    raise travel_to_d3m
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test34 () {
	    enter
	    raise travel_to_d1m
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test35 () {
	    enter
	    raise travel_to_b1
	    raise toggle
	    raise travel_to_d1m
	    raise travel_to_d1p
	    assert is_active
	    exit
	    assert !is_active
	}

}