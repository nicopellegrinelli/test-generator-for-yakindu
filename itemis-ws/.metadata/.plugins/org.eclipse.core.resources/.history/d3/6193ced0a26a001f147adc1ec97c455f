testclass StatechartWithVarsSimplifiedTest for statechart StatechartWithVars {
	@Test
	operation test01 () {
	    enter
	    raise start
	    raise res
	    assert is_active
	}

	@Test
	operation test02 () {
	    enter
	    raise start
	    raise stop
	    assert is_active
	}

	@Test
	operation test03 () {
	    enter
	    raise start
	    raise dec
	    assert is_active
	}

	@Test
	operation test04 () {
	    enter
	    raise start
	    raise start
	    assert is_active
	}

	@Test
	operation test05 () {
	    enter
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test06 () {
	    enter
	    raise start
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test07 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test08 () {
	    enter
	    raise start
	    assert is_active
	    assert active (StatechartWithVars.main_region.StateB)
	}

	@Test
	operation test09 () {
	    enter
	    assert is_active
	    assert active (StatechartWithVars.main_region.StateA)
	}

	@Test
	operation test10 () {
	}

	@Test
	operation test11 () {
	    assert !active (StatechartWithVars.main_region.StateB)
	}

	@Test
	operation test12 () {
	    assert !active (StatechartWithVars.main_region.StateA)
	}

	@Test
	operation test13 () {
	    enter
	    assert is_active
	}

	@Test
	operation test14 () {
	    assert !is_active
	}

	@Test
	operation test15 () {
	    exit
	    assert !is_active
	}

	@Test
	operation test16 () {
	    raise stop
	    assert !is_final
	}

	@Test
	operation test17 () {
	    assert !is_active
	    enter
	    raise start
	    raise inc
	    assert is_active
	}

	@Test
	operation test18 () {
	    assert !is_final
	    assert !is_active
	}

}