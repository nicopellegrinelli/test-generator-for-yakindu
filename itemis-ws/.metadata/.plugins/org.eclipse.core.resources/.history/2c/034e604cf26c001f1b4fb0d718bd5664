testclass TrafficLight_WTSimplifiedTest for statechart TrafficLight_WT {
	@Test
	operation test00 () {
	    enter
	    proceed 60s
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test01 () {
	    enter
	    proceed 60s
	    raise pedestrian
	    triggerWithoutEvent
	    assert is_active
	}

	@Test
	operation test02 () {
	    enter
	    raise pedestrian
	    assert is_active
	}

	@Test
	operation test03 () {
	    exit
	    assert !is_final
	}

	@Test
	operation test04 () {
	    enter
	    proceed 60s
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test05 () {
	    enter
	    proceed 60s
	    raise pedestrian
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test06 () {
	    enter
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test07 () {
	    enter
	    proceed 60s
	    assert is_active
	    assert active (TrafficLight_WT.main_region.green)
	}

	@Test
	operation test08 () {
	    enter
	    proceed 60s
	    raise pedestrian
	    assert is_active
	    assert active (TrafficLight_WT.main_region.pending)
	}

	@Test
	operation test09 () {
	    enter
	    assert is_active
	    assert active (TrafficLight_WT.main_region.red)
	}

	@Test
	operation test10 () {
	}

	@Test
	operation test11 () {
	    assert !active (TrafficLight_WT.main_region.green)
	}

	@Test
	operation test12 () {
	    assert !active (TrafficLight_WT.main_region.yellow)
	}

	@Test
	operation test13 () {
	    assert !active (TrafficLight_WT.main_region.pending)
	}

	@Test
	operation test14 () {
	    assert !active (TrafficLight_WT.main_region.red)
	}

	@Test
	operation test15 () {
	    proceed 60s
	    assert !is_active
	}

	@Test
	operation test16 () {
	    enter
	    proceed 60s
	    raise pedestrian
	    proceed 60s
	    triggerWithoutEvent
	    assert !is_final
	}

	@Test
	operation test17 () {
	    assert !is_active
	}

	@Test
	operation test20 () {
	    assert !is_final
	    assert !is_active
	}

	@Test
	operation test21 () {
	    enter
	    proceed 60s
	    raise pedestrian
	    proceed 60s
	    assert is_active
	    exit
	    assert !is_active
	}

	@Test
	operation test22 () {
	    assert !is_active
	    enter
	    assert is_active
	}

}