				raiseSetYellow(true);
				timeEvents[6] = false;
				enterSequence_main_region_Controller_r1_Police_Mode_r1_Yellow_on_default();
				main_region_Controller_r1_Police_Mode_react(0l);
				transitioned_after = 0l;
			} else {
