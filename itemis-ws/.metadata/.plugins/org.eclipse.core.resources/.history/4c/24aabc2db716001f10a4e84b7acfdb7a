		if (historyVector[0] != State.$NULLSTATE$) {
			shallowEnterSequence_main_region_Awake_walkAndAvoidObstacles();
		} else {
			enterSequence_main_region_Awake_walkAndAvoidObstacles_WalkStraight_default();
