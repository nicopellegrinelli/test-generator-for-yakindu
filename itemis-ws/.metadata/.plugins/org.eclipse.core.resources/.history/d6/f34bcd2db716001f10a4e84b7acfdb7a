			exitSequence_modes_Automatic_modes_Light_Sensing_r1_Dark();
			break;
		case MODES_AUTOMATIC_MODES_LIGHT_SENSING_R1_GETTING_DARK:
			exitSequence_modes_Automatic_modes_Light_Sensing_r1_Getting_Dark();
